<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_today">

    <fragment
        android:id="@+id/nav_today"
        android:name="com.csd051.superiora.ui.home.today_schedule.TodayScheduleFragment"
        android:label="@string/today_schedule"
        tools:layout="@layout/fragment_today_schedule" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.csd051.superiora.ui.home.slideshow.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />

    <activity
        android:id="@+id/nav_settings"
        android:name="com.csd051.superiora.ui.setting.SettingsActivity"
        android:label="SettingsActivity" />

    <fragment
        android:id="@+id/nav_task"
        android:name="com.csd051.superiora.ui.home.yourtask.YourTaskFragment"
        android:label= "@string/your_task"
        tools:layout="@layout/fragment_your_task" />

    <fragment
        android:id="@+id/nav_android"
        android:name="com.csd051.superiora.ui.home.roadmaps.RoadmapsFragment"
        android:label="@string/android"
        tools:layout="@layout/fragment_roadmaps" >
        <argument
            android:name="courseId"
            app:argType="integer"
            android:defaultValue="1" />
    </fragment>

    <fragment
        android:id="@+id/nav_angular"
        android:name="com.csd051.superiora.ui.home.roadmaps.RoadmapsFragment"
        android:label="@string/angular"
        tools:layout="@layout/fragment_roadmaps" >
        <argument
            android:name="courseId"
            app:argType="integer"
            android:defaultValue="2" />
    </fragment>

    <fragment
        android:id="@+id/nav_backend"
        android:name="com.csd051.superiora.ui.home.roadmaps.RoadmapsFragment"
        android:label="@string/backend"
        tools:layout="@layout/fragment_roadmaps" >
        <argument
            android:name="courseId"
            app:argType="integer"
            android:defaultValue="3" />
    </fragment>

    <fragment
        android:id="@+id/nav_dba"
        android:name="com.csd051.superiora.ui.home.roadmaps.RoadmapsFragment"
        android:label="@string/dba"
        tools:layout="@layout/fragment_roadmaps" >
        <argument
            android:name="courseId"
            app:argType="integer"
            android:defaultValue="4" />
    </fragment>

    <fragment
        android:id="@+id/nav_devops"
        android:name="com.csd051.superiora.ui.home.roadmaps.RoadmapsFragment"
        android:label="@string/devops"
        tools:layout="@layout/fragment_roadmaps" >
        <argument
            android:name="courseId"
            app:argType="integer"
            android:defaultValue="5" />
    </fragment>

    <fragment
        android:id="@+id/nav_frontend"
        android:name="com.csd051.superiora.ui.home.roadmaps.RoadmapsFragment"
        android:label="@string/frontend"
        tools:layout="@layout/fragment_roadmaps" >
        <argument
            android:name="courseId"
            app:argType="integer"
            android:defaultValue="6" />
    </fragment>

    <fragment
        android:id="@+id/nav_go"
        android:name="com.csd051.superiora.ui.home.roadmaps.RoadmapsFragment"
        android:label="@string/go"
        tools:layout="@layout/fragment_roadmaps" >
        <argument
            android:name="courseId"
            app:argType="integer"
            android:defaultValue="7" />
    </fragment>

    <fragment
        android:id="@+id/nav_java"
        android:name="com.csd051.superiora.ui.home.roadmaps.RoadmapsFragment"
        android:label="@string/java"
        tools:layout="@layout/fragment_roadmaps" >
        <argument
            android:name="courseId"
            app:argType="integer"
            android:defaultValue="8" />
    </fragment>

