        tools:layout="@layout/fragment_roadmaps" >
        <argument
            android:name="courseId"
            app:argType="integer"
            android:defaultValue="9" />
    </fragment>

    <fragment
        android:id="@+id/nav_react"
        android:name="com.csd051.superiora.ui.home.roadmaps.RoadmapsFragment"
        android:label="@string/react"
        tools:layout="@layout/fragment_roadmaps" >
        <argument
            android:name="courseId"
            app:argType="integer"
            android:defaultValue="10" />
    </fragment>
</navigation>